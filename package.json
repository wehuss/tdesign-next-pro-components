{
  "name": "tdesign-next-pro-components",
  "version": "0.0.1-alpha.8",
  "description": "Enhanced components based on TDesign Vue Next",
  "keywords": [
    "component",
    "library",
    "tdesign",
    "ui",
    "vue",
    "vue3"
  ],
  "homepage": "https://github.com/wehuss/tdesign-pro-components#readme",
  "bugs": {
    "url": "https://github.com/wehuss/tdesign-pro-components/issues"
  },
  "license": "MIT",
  "author": "TDesign Pro Components Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/wehuss/tdesign-pro-components.git"
  },
  "style": "dist/tdesign-next-pro-components.css",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./style.css": "./dist/tdesign-next-pro-components.css",
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "dev": "vite --config playground/vite.config.ts playground",
    "dev:docs": "vitepress dev docs",
    "dev:lib": "vite build --config vite.config.lib.ts --watch",
    "build": "pnpm run build:lib && pnpm run build:types",
    "build:lib": "vite build --config vite.config.lib.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "build:docs": "vitepress build docs",
    "preview": "vite preview",
    "preview:docs": "vitepress preview docs",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "format": "pnpm oxfmt .",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage",
    "prepare": "husky",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:all": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "version:patch": "standard-version --release-as patch",
    "version:minor": "standard-version --release-as minor",
    "version:major": "standard-version --release-as major",
    "version:alpha": "standard-version --prerelease alpha",
    "version:beta": "standard-version --prerelease beta",
    "release": "pnpm run build && npm publish",
    "release:dry": "standard-version --dry-run"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "tdesign-icons-vue-next": "^0.3.6",
    "tdesign-vue-next": "^1.15.2",
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.5",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vitest/coverage-v8": "^4.0.15",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "conventional-changelog-cli": "^5.0.0",
    "fast-check": "^4.3.0",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "less": "^4.4.0",
    "oxfmt": "^0.23.0",
    "oxlint": "^1.38.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-esbuild": "^6.2.1",
    "standard-version": "^9.5.0",
    "typescript": "~5.8.3",
    "vite": "8.0.0-beta.0",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "2.0.0-alpha.15",
    "vitest": "^4.0.15",
    "vue-tsc": "^2.2.12"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  }
}
